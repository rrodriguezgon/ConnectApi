{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Elasticsearch": {
    "UriString": "http://127.0.0.1:9200",
    "IndexFormat": "local-ProviderUrlScan-{0:yyyy.MM.dd}",
    "MinimumLogEventLevel": "Verbose",
    "AutoRegisterTemplate": true
  }
}